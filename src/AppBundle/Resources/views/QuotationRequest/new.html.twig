{% extends "AppBundle:Admin:base.html.twig" %}


 {% block body %}

    <div class="container">
        <div class="page-header">
            <h1>Creation d'une demande de devis</h1>
        </div>
        {{ form_start(form) }}

        {{ form_errors(form) }}
        {{ form_row(form.status) }}
        {{ form_row(form.vehicleModel) }}
        {{ form_row(form.problemDescription) }}
        {{ form_row(form.hasShelter) }}
        <div class="btn_place"><strong>Services</strong></div>
        <div class="well">
            <ul class="service_relations"
                data-prototype="{{ form_widget(form.quotationRequestServiceRelations.vars.prototype)|e }}">
            </ul>
        </div>

        {{ form_row(form.quotationRequestServiceRelations, { 'disabled':'disabled' }) }}

        {{ form_end(form) }}

    </div>

{% endblock %}

{% block ending_javascripts %}
    {% javascripts
    '@AppBundle/Resources/public/js/service_relation_handler.js' %}
    <script type="text/javascript" src="{{ asset_url }}"></script>
    {% endjavascripts %}
{% endblock %}

